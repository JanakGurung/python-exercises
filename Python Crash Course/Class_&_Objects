class Restaurant:
    def __init__(self, restaurant_name, cuisine_type):
        self.restaurant_name = restaurant_name
        self.cuisine_type = cuisine_type
    
    def describe_restaurant(self):
        print(f"Our Restaurant name is {self.restaurant_name} and we serve {self.cuisine_type} cuisine at our outlet.")
    
restaurant1 = Restaurant("Parmesan", "Italian")
restaurant2 = Restaurant("Ek tadka 100 Jhatka", "Indian")
restaurant3 = Restaurant("Chippychips", "English")

restaurant1.describe_restaurant()
restaurant2.describe_restaurant()
restaurant3.describe_restaurant()